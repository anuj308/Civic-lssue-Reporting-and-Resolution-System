{
  "name": "civic-issue-mobile",
  "version": "1.0.0",
  "description": "Mobile app for Civic Issue Reporting System",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "install:clean": "rm -rf node_modules && npm install",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "expo": "~54.0.0",
    "expo-audio": "^1.0.13",
    "expo-av": "~16.0.7",
    "expo-camera": "~17.0.7",
    "expo-constants": "~18.0.8",
    "expo-device": "~8.0.7",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.14",
    "expo-font": "~14.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.11",
    "expo-status-bar": "~3.0.8",
    "expo-video": "^3.0.11",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-hook-form": "^7.45.2",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-maps": "1.20.1",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.9.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-skeleton-content": "^1.0.25",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^10.0.0",
    "react-native-worklets": "^0.5.1",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tsconfig/react-native": "^3.0.6",
    "@types/react": "~18.2.14",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.6.2",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true
